spring:
  datasource:
    url: jdbc:postgresql://localhost:5432/toy_db
    username: postgres
    password: postgres
    driver-class-name: org.postgresql.Driver
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
    properties:
      hibernate:
        format_sql: true
        dialect: org.hibernate.dialect.PostgreSQLDialect

server:
  port: 8080

# Copy this file to application-dev.yml and adjust values as needed 